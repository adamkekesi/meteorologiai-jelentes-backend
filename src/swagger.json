                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "A megadott felhasználó (id) nem létezik."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },

