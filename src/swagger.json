        "/measurements/{offset}/{limit}/{field}/{filter}": {
            "get": {
                "tags": ["Measurement"],
                "summary": "Get paginated measurement with filters",
                "parameters": [
                    {
                        "in": "path",
                        "name": "offset",
                        "description": "Offset the data",
                        "schema": {
                            "type": "number",
                            "example": 0
                        },
                        "required": true
                    },
                    {
                        "in": "path",
                        "name": "limit",
                        "description": "Number of records the API sending",
                        "schema": {
                            "type": "number",
                            "example": 2
                        },
                        "required": true
                    },
                    {
