        "/users/{id}": {
            "get": {
                "tags": ["users"],
                "summary": "Egy felhasználó lekérdezése.",
                "description": "Az id paraméterben megadott felhasználó adatait kérdezhetjük le.",
                "parameters": [
                    {
                        "name": "id",
