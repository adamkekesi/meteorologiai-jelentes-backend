                    {
                        "name": "id",
                        "description": "A PATCH metódussal módosítandó felhasználó azonosítója (PK).",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "example": "61b5e9c0f39e4edcf5b8a3b9"
                        },
                        "required": true
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "$ref": "#/components/schemas/user"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK (A módosított felhasználó adatokkal tér vissza.)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/user"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "A megadott felhasználó (id) nem létezik."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": ["users"],
                "summary": "felhasználó törlése.",
                "description": "A paraméterben megadott felhasználót törli a végpont.",
                "parameters": [
                    {
                        "name": "id",
                        "description": "A törlendő felhasználó azonosítója (PK).",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "example": "61b5f28df39e4edcf5b8a3e7"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK (A felhasználó törölve.)",
