                                "schema": {
                                    "description": "An array of cities",
                                    "type": "object",
                                    "properties": {
                                        "cities": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/City"
                                            }
                                        }
                                    }
                                },
                                "example": {
                                    "cities": [
                                        {
                                            "_id": 1,
                                            "code": "BP",
                                            "fullName": "Budapest"
                                        },
                                        {
                                            "_id": 2,
                                            "code": "DE",
                                            "fullName": "Debrecen"
                                        }
                                    ]
                                }
                            }
                        }
